a = 15000
b = 45000
c = 65000

ta = 0.1
tb = 0.05
tc = 0.025

y = 0
while a < b:
    a = a+(a*ta)
    b = b+(b*tb)
    y += 1

print(f'A população de A se igualará e ultrapassará a população de B em {y} anos')

a = 15000


y2 = 0
while a <= c+(c*0.23):
    a = a+(a*ta)
    c = c+(c*tc)
    y2 += 1

print(f'A população de A ultrapassará a população de C em 23% em {y2} anos')


